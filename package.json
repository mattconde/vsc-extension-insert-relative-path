{
  "name": "insert-relative-path",
  "displayName": "insert-relative-path",
  "description": "",
  "version": "0.0.1",
  "publisher": "mattconde",
  "repository": {
    "type": "git",
    "url": "https://github.com/mattconde/vsc-extension-insert-relative-path.git"
  },
  "engines": {
    "vscode": "^1.28.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:extension.importRelativePath"
  ],
  "main": "./extension",
  "contributes": {
    "commands": [
      {
        "command": "extension.importRelativePath",
        "title": "Slap in relative path"
      }
    ],
    "menus": {
      "editor/title/context": [
        {
          "command": "extension.importRelativePath",
          "group": "1_modification",
          "when": "resourceLangId !== markdown"
        }
      ],
      "explorer/context": [
        {
          "command": "extension.importRelativePath",
          "group": "1_modification",
          "when": "resourceLangId !== markdown"
        }
      ]
    }
  },
  "scripts": {
    "postinstall": "node ./node_modules/vscode/bin/install",
    "test": "node ./node_modules/vscode/bin/test"
  },
  "devDependencies": {
    "vscode": "^1.1.21",
    "eslint": "^4.11.0"
  }
}
